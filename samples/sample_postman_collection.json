{
  "info": {
    "name": "API Test Collection",
    "description": "A comprehensive test collection for API discovery parser",
    "version": {
      "major": 2,
      "minor": 1,
      "patch": 0
    },
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "exportedAt": "2024-01-15T10:30:00.000Z",
    "exportedBy": "Postman/10.0.0"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "https://api.example.com",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "your-api-key-here",
      "type": "string"
    },
    {
      "key": "auth_token",
      "value": "bearer-token-here",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{auth_token}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Users API",
      "item": [
        {
          "name": "Get All Users",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "X-API-Version",
                "value": "v1"
              }
            ],
            "url": {
              "raw": "{{base_url}}/users?page=1&limit=10",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "users"
              ],
              "query": [
                {
                  "key": "page",
                  "value": "1",
                  "description": "Page number"
                },
                {
                  "key": "limit",
                  "value": "10",
                  "description": "Number of items per page"
                }
              ]
            }
          },
          "response": [
            {
              "name": "Success Response",
              "originalRequest": {
                "method": "GET",
                "header": [],
                "url": {
                  "raw": "{{base_url}}/users?page=1&limit=10",
                  "host": [
                    "{{base_url}}"
                  ],
                  "path": [
                    "users"
                  ],
                  "query": [
                    {
                      "key": "page",
                      "value": "1"
                    },
                    {
                      "key": "limit",
                      "value": "10"
                    }
                  ]
                }
              },
              "status": "OK",
              "code": 200,
              "_postman_previewlanguage": "json",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "cookie": [],
              "body": "{\n  \"users\": [\n    {\n      \"id\": 1,\n      \"name\": \"John Doe\",\n      \"email\": \"john@example.com\"\n    }\n  ],\n  \"total\": 1,\n  \"page\": 1\n}"
            }
          ]
        },
        {
          "name": "Create User",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Jane Doe\",\n  \"email\": \"jane@example.com\",\n  \"age\": 30,\n  \"active\": true\n}"
            },
            "url": {
              "raw": "{{base_url}}/users",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "users"
              ]
            }
          },
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "console.log('Pre-request script executed');",
                  "pm.environment.set('request_timestamp', new Date().toISOString());"
                ]
              }
            },
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "pm.test('Status code is 201', function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test('Response has user id', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('id');",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Get User by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/users/:id",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "users",
                ":id"
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Products API",
      "auth": {
        "type": "apikey",
        "apikey": [
          {
            "key": "key",
            "value": "X-API-Key",
            "type": "string"
          },
          {
            "key": "value",
            "value": "{{api_key}}",
            "type": "string"
          },
          {
            "key": "in",
            "value": "header",
            "type": "string"
          }
        ]
      },
      "item": [
        {
          "name": "Get Products",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/products?category=electronics&min_price=100",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "products"
              ],
              "query": [
                {
                  "key": "category",
                  "value": "electronics",
                  "description": "Product category"
                },
                {
                  "key": "min_price",
                  "value": "100",
                  "description": "Minimum price filter"
                }
              ]
            }
          }
        },
        {
          "name": "Create Product",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Smartphone\",\n  \"price\": 599.99,\n  \"category\": \"electronics\",\n  \"in_stock\": true,\n  \"specifications\": {\n    \"brand\": \"TechCorp\",\n    \"model\": \"X1\",\n    \"color\": \"black\"\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/products",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "products"
              ]
            }
          }
        },
        {
          "name": "Upload Product Image",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "image",
                  "type": "file",
                  "src": "/path/to/image.jpg"
                },
                {
                  "key": "product_id",
                  "value": "123",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/products/upload",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "products",
                "upload"
              ]
            }
          }
        }
      ]
    },
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "username",
                  "value": "user@example.com",
                  "type": "text"
                },
                {
                  "key": "password",
                  "value": "password123",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/auth/login",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "auth",
                "login"
              ]
            }
          },
          "auth": {
            "type": "basic",
            "basic": [
              {
                "key": "username",
                "value": "admin",
                "type": "string"
              },
              {
                "key": "password",
                "value": "admin123",
                "type": "string"
              }
            ]
          }
        },
        {
          "name": "OAuth2 Token",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/x-www-form-urlencoded"
              }
            ],
            "body": {
              "mode": "urlencoded",
              "urlencoded": [
                {
                  "key": "grant_type",
                  "value": "client_credentials",
                  "type": "text"
                },
                {
                  "key": "client_id",
                  "value": "your-client-id",
                  "type": "text"
                },
                {
                  "key": "client_secret",
                  "value": "your-client-secret",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/oauth/token",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "oauth",
                "token"
              ]
            }
          },
          "auth": {
            "type": "oauth2",
            "oauth2": [
              {
                "key": "clientId",
                "value": "your-client-id",
                "type": "string"
              },
              {
                "key": "clientSecret",
                "value": "your-client-secret",
                "type": "string"
              },
              {
                "key": "accessToken",
                "value": "access-token-here",
                "type": "string"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "File Operations",
      "item": [
        {
          "name": "Upload File",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "file",
              "file": {
                "src": "/path/to/document.pdf",
                "content": "base64-encoded-content"
              }
            },
            "url": {
              "raw": "{{base_url}}/files/upload",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "files",
                "upload"
              ]
            }
          }
        },
        {
          "name": "Download File",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/files/{file_id}/download",
              "host": [
                "{{base_url}}"
              ],
              "path": [
                "files",
                "{file_id}",
                "download"
              ]
            }
          }
        }
      ]
    }
  ]
} 